@model dynamic
<!DOCTYPE html>
<html>
<head>
    <style>

        /* Set the size of the div element that contains the map */
        #map {
            height: 500px; /* The height is 400 pixels */
            width: 100%; /* The width is the width of the web page */
        }

        html, body {
            height: 80%;
            margin: 0;
            padding: 0;
        }
    </style>
</head>
<body>
    <h2>Dashboard</h2>
    <hr />
    <input id="Weather" type="checkbox" value="Weather"/><label for="Weather">Weather</label>
    <input id="Temperature" type="checkbox" value="Temperature"/><label for="Temperature">Temperature</label>
    <input id="Haze" type="checkbox" value="Haze"/><label for="Haze">Haze</label>
    <input id="Emergency" type="checkbox" value="Emergency"/><label for="Emergency">Emergency</label>
    <input id="Dengue" type="checkbox" value="Dengue"/><label for="Dengue">Dengue</label>
    <div id="map"></div>
    @*
        <!--The div element for the map -->
        <div id="map"></div>
            <script>
                // Initialize and add the map
                function initMap() {
                    // The location of Singapore
                    var singapore = { lat: 1.3521, lng: 103.8198 };
                    // The map, centered at Singapore
                    var map = new google.maps.Map(
                        document.getElementById('map'), { zoom: 11, center: singapore });

                }
            </script>
